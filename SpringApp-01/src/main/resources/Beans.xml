<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
    <bean id="credit" class="ai.inueron.beans.creditCard"></bean>
    <bean id="debit" class="ai.inueron.beans.debitCard"></bean>
    <bean id="upi" class="ai.inueron.beans.upiPay"></bean>
    <bean id="paymentprocess" class="ai.inueron.beans.PaymentProcess">
    <property name="selectPaymentProcess" ref="credit"></property>
    </bean>
</beans>