package ai.merit.HB.model;

import javax.persistence.Entity;
import javax.persistence.Id;

import org.hibernate.annotations.DynamicInsert;

@Entity
@DynamicInsert(value=true)
public class HBStudnet {

	@Id
	private int id;
	private String name;
	private int age;
	private String city;
	
	private HBStudnet() {
		System.out.println("Internally called by hibernate");
	}
	
	
	
}
